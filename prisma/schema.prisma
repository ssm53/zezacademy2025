generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Applicant {
  id               Int     @id @default(autoincrement())
  fullname         String?
  email            String
  whatsapp         String?
  reason           String?
  codingexperience String?
  stageofcareer    String?
  fullorpart       String?
  remoteoronsite   String?
}

model StarterPack {
  id               Int     @id @default(autoincrement())
  fullname         String?
  email            String
  whatsapp         String?
}
